{
  "private": true,
  "name": "galaxytester0982",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "export:kla:all": "node ./tools/export-kla-cli.mjs",
    "export:kla:only": "node ./tools/export-kla-cli.mjs --only=%only%",
    "postinstall": "npm run build:main && npm run build:client && npm run build:server",
    "build:main": "vite build",
    "build:client": "cd src/client && vite build",
    "build:server": "cd src/server && vite build",
    "build": "npm run build:main && npm run build:client && npm run build:server",
    "check": "npm run type-check && npm run lint:fix && npm run prettier",
    "deploy": "npm run build && devvit upload",
    "dev": "concurrently -k -p \"[{name}]\" -n \"MAIN,CLIENT,SERVER,DEVVIT\" -c \"red,blue,green,magenta\" \"npm run dev:main\" \"npm run dev:client\" \"npm run dev:server\" \"npm run dev:devvit\"",
    "dev:main": "vite build --watch",
    "dev:fast": "concurrently -k -p \"[{name}]\" -n \"CLIENT,SERVER\" -c \"blue,green\" \"npm run dev:client\" \"npm run dev:server\"",
    "preview:client": "cd src/client && vite preview --port 5173 --strictPort",
    "dev:client": "cd src/client && vite build --watch --mode development",
    "dev:devvit": "wait-on \"dist/main.cjs\" \"dist/client/index.html\" \"dist/server/index.cjs\" tcp:127.0.0.1:3000 && dotenv -e .env -- devvit playtest",
    "dev:server": "concurrently -k -p \"[{name}]\" -n \"BUILD,RUN\" -c \"yellow,green\" \"npm:watch:server\" \"npm:start:server\"",
    "watch:server": "cd src/server && vite build --watch",
    "start:server": "nodemon --delay 500ms dist/server/index.cjs --watch dist/server",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "login": "devvit login --copy-paste",
    "launch": "npm run build && npm run deploy && devvit publish",
    "prettier": "prettier-package-json --write ./package.json && prettier --write .",
    "type-check": "tsc --build",
    "generate-powerups": "node tools/generate-powerups.cjs",
    "export:aseprite:auto-cannon": "node tools/export-aseprite-from-png.mjs",
    "export:aseprite:auto-cannon:cli": "node tools/export-aseprite-cli.mjs",
    "export:aseprite:all": "node tools/export-aseprite-cli.mjs",
    "export:aseprite:shield": "node tools/export-aseprite-cli.mjs --only=invincibilityShield"
  },
  "dependencies": {
    "@devvit/web": "0.12.0",
    "devvit": "0.12.0",
    "express": "5.1.0",
    "phaser": "^3.88.2"
  },
  "devDependencies": {
    "@eslint/js": "9.23.0",
    "@types/express": "5.0.1",
    "concurrently": "9.1.2",
    "dotenv-cli": "8.0.0",
    "eslint": "9.23.0",
    "globals": "15.15.0",
    "imagemagick": "^0.1.3",
    "nodemon": "^3.1.10",
    "prettier": "3.5.3",
    "prettier-package-json": "2.8.0",
    "terser": "5.39.0",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0",
    "vite": "6.2.4",
    "vitest": "3.1.1",
    "wait-on": "7.2.0"
  }
}
